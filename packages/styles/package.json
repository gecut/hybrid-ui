{
  "name": "@gecut/styles",
  "version": "2.8.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gecut/hybrid-ui",
    "directory": "packages/styles"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./css": {
      "default": "./src/index.css"
    },
    "./*.css": {
      "default": "./src/*.css"
    },
    "./*.js": {
      "default": "./*.js",
      "types": "./*.d.ts"
    }
  },
  "files": [
    "**/*.{js,mjs,cjs,map,d.ts,html,md,css}"
  ],
